{
    "extends": [
        "next/core-web-vitals",
        "prettier"
    ],
    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "prettier/prettier": [
            "warn",
            {
                "endOfLine": "auto",
                "singleQuote": true
            }
        ],
        "react/no-children-prop": "off"
    }
}
